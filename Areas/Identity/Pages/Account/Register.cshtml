@page
@model AskeOgViktorProjekt.Areas.Identity.Pages.Account.RegisterModel
@{
    ViewData["Title"] = "Register";
}

<h1>@ViewData["Title"]</h1>

<form id="registerForm" method="post">
    @Html.AntiForgeryToken()

    <label asp-for="NewUser.Name">Name</label><br />
    <input id="name" type="text" asp-for="NewUser.Name" /><br />
    <span asp-validation-for="NewUser.Name" class="text-danger"></span><br />

    <label asp-for="NewUser.Password">Password</label><br />
    <input id="password" type="password" asp-for="NewUser.Password" /><br />
    <span asp-validation-for="NewUser.Password" class="text-danger"></span><br />

    <input type="submit" value="Register" />
</form>
